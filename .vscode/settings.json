{
    "dotfiles.repository": "https://github.com/horatjp/dotfiles",
    "dotfiles.targetPath": "~/dotfiles",
    "dotfiles.installCommand": "~/dotfiles/setup-devcontainer.sh",
    "editor.codeActionsOnSave": {
